{
  "name": "maxhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint --cache --cache-strategy content --cache-location .eslintcache .",
    "lint:fix": "eslint --fix --cache --cache-strategy content --cache-location .eslintcache .",
    "lint:file": "eslint --cache --cache-strategy content --cache-location .eslintcache",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest tests/unit",
    "test:integration": "node --env-file=.env ./node_modules/vitest/vitest.mjs run tests/integration",
    "test:e2e": "node --env-file=.env ./node_modules/playwright/cli.js test",
    "test:e2e:ui": "node --env-file=.env ./node_modules/playwright/cli.js test --ui",
    "check:tailwind:canonical": "node scripts/tailwind-canonicalize.mjs --check",
    "fix:tailwind:canonical": "node scripts/tailwind-canonicalize.mjs --write",
    "lint:tailwind": "npm run check:tailwind:canonical",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "create:user": "node --env-file=.env create-user.mjs"
  },
  "engines": {
    "node": "24.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.988.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
